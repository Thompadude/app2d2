<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sw.app2d2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:debuggable="true"
        android:icon="@drawable/icon_app"
        android:label="@string/app_name">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AboutActivity" />
        <activity android:name=".forcemeter.ForceMeterActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".characters.CharacterActivity" />
        <activity android:name=".quiz.QuizNewGameActivity" />
        <activity android:name=".quiz.QuizActivity" />

        <service
            android:name=".characters.json.CharacterService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".quiz.QuizResultActivity"></activity>
    </application>

</manifest>
