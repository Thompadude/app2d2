<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sw.app2d2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:debuggable="true"
        android:icon="@drawable/icon_r2d2"
        android:label="@string/app_name">
        <activity
            android:name="sw.app2d2.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="sw.app2d2.AboutActivity" />
        <activity android:name="sw.app2d2.forcemeter.ForceMeterActivity" />
        <activity android:name="sw.app2d2.characters.ProfileActivity" />
    </application>
</manifest>